type Bird(name) -> {
      speed := 1.65
      func fly() -> {
          println('flying')
      }
}
unit StaticBird -> {
     some_static_field := 'world'
     func fly() -> {
        i := 0
        while (i < 1000000) {
            println('Hello world!')
            println(i)
            i = i + 1
            println(self.some_static_field)
        }
     }
}
example = new Bird('kea')
example.fly()

func example() -> {
   println('flying')
}
